{
  "name": "cocos-build",
  "version": "1.2.2",
  "private": "false",
  "description": "Cocos Creator Build Action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test",
    "postall": "git add .",
    "prepare": "husky install",
    "release": "standard-version --no-verify",
    "startrelease": "git flow release start release",
    "finishrelease": "export GIT_MERGE_AUTOEDIT=no && git flow release finish release -n -p && unset GIT_MERGE_AUTOEDIT"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miggene/cocos-build.git"
  },
  "keywords": [
    "actions",
    "cocos creator",
    "build"
  ],
  "author": "miggene",
  "license": "MIT",
  "dependencies": {
    "@actions/artifact": "^0.5.2",
    "@actions/core": "^1.4.0",
    "@actions/exec": "^1.1.0",
    "@actions/glob": "^0.2.0",
    "@actions/tool-cache": "^1.7.1",
    "@types/axios": "^0.14.0",
    "axios": "^0.21.4"
  },
  "devDependencies": {
    "@types/node": "^16.3.3",
    "@typescript-eslint/parser": "^4.28.3",
    "@vercel/ncc": "^0.29.0",
    "eslint": "^7.31.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.3.6",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "js-yaml": "^4.1.0",
    "prettier": "2.2.1",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.1.3"
  }
}
